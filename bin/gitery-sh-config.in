#!/bin/sh

if [ -z "${__included_gitery_sh_config-}" ]; then
__included_gitery_sh_config=1

ACL_DIR='@ACL_DIR@'
CMD_DIR='@CMD_DIR@'
CONF_DIR='@CONF_DIR@'
EMAIL_ALIASES='@EMAIL_ALIASES@'
EMAIL_DIR='@EMAIL_DIR@'
EMAIL_DOMAIN='@EMAIL_DOMAIN@'
GEARS_DIR='@GEARS_DIR@'
GITWEB_URL='@GITWEB_URL@'
GIT_TEMPLATE_DIR='@GIT_TEMPLATE_DIR@'
HOOKS_DIR='@HOOKS_DIR@'
PACKAGES_EMAIL='@PACKAGES_EMAIL@'
PEOPLE_DIR='@PEOPLE_DIR@'
RUNTIME_DIR='@RUNTIME_DIR@'
SRPMS_DIR='@SRPMS_DIR@'
STATE_DIR='@STATE_DIR@'
MAINTAINERS_GROUP='@MAINTAINERS_GROUP@'
USERS_GROUP='@USERS_GROUP@'
USER_PREFIX='@USER_PREFIX@'

PROJECT_INITIAL_REGEXP='[A-Za-z0-9]'
PROJECT_NAME_REGEXP="$PROJECT_INITIAL_REGEXP[-A-Za-z0-9_.]+"
USER_NAME_REGEXP='[a-z][a-z_0-9]+'

if [ -s "$CONF_DIR/repo/.default" ]; then
	. "$CONF_DIR/repo/.default"
fi
if [ -n "${GITERY_REPO_NAME-}" ] &&
   [ -s "$CONF_DIR/repo/${GITERY_REPO_NAME-}" ]; then
	. "$CONF_DIR/repo/${GITERY_REPO_NAME-}"
fi

fi #__included_gitery_sh_config
